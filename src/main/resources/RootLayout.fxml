<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:id="pane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.RootLayoutController">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#onAntropologicalSelected" text="Antropologicals" />
              <MenuItem mnemonicParsing="false" onAction="#onCoachesSelected" text="Coaches" />
              <MenuItem mnemonicParsing="false" onAction="#onCoachHistorySelected" text="Coach History" />
              <MenuItem mnemonicParsing="false" onAction="#onCompetitionSelected" text="Competitions" />
              <MenuItem mnemonicParsing="false" onAction="#onGymSelected" text="Gyms" />
              <MenuItem mnemonicParsing="false" onAction="#onParticipantSelected" text="Participants" />
              <MenuItem mnemonicParsing="false" onAction="#onSponsorsSelected" text="Sponsors" />
              <MenuItem mnemonicParsing="false" onAction="#onSportClubSelected" text="Sport Clubs" />
              <MenuItem mnemonicParsing="false" onAction="#onTransportSelected" text="Transports" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
</BorderPane>
